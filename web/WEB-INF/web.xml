<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>controller.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>blogtype</servlet-name>
        <servlet-class>controller.blogtype</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddTimeline</servlet-name>
        <servlet-class>controller.AddTimeline</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_data</servlet-name>
        <servlet-class>controller.load_data</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_blog</servlet-name>
        <servlet-class>controller.add_blog</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>blogtype</servlet-name>
        <url-pattern>/blogtype.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTimeline</servlet-name>
        <url-pattern>/AddTimeline.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_data</servlet-name>
        <url-pattern>/load_data.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_blog</servlet-name>
        <url-pattern>/add_blog.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
